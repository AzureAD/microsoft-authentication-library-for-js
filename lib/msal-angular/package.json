{
  "name": "@azure/msal-angular",
  "version": "2.0.0-alpha.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "npm run build:common && npm run build:browser && npm run build",
    "build:browser": "cd ../msal-browser && npm run build",
    "build:common": "cd ../msal-common && npm run build",
    "build:pack": "npm run build && cd dist && npm pack",
    "deploy": "npm run build:all && npm publish dist",
    "test": "ng test",
    "test:coverage": "npm test",
    "lint": "cd ../../ && npm run lint:angular",
    "lint:fix": "npm run lint -- -- --fix",
    "e2e": "ng e2e",
    "prepack": "npm run build:all",
    "preinstall": "npx npm-force-resolutions",
    "prepublishOnly": "echo Use npm run deploy && exit 1"
  },
  "main": "./dist/bundles/azure-msal-angular.umd.js",
  "typings": "./dist/azure-msal-angular.d.ts",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/animations": "~10.0.9",
    "@angular/cli": "~10.0.6",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/compiler-cli": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@azure/msal-browser": "^2.11.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "npm-force-resolutions": "0.0.3",
    "protractor": "~7.0.0",
    "rxjs": "~6.5.5",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.3"
  },
  "peerDependencies": {
    "@angular/common": "9 - 11",
    "@angular/core": "9 - 11",
    "@azure/msal-browser": "^2.11.0",
    "rxjs": "^6.0.0"
  },
  "resolutions": {
    "ini": "^1.3.6",
    "object-path": "0.11.5"
  }
}
