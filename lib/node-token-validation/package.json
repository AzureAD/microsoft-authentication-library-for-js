{
  "name": "@azure/node-token-validation",
  "author": {
    "name": "Microsoft",
    "email": "nugetaad@microsoft.com",
    "url": "https://www.microsoft.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AzureAD/microsoft-authentication-library-for-js.git"
  },
  "version": "1.0.0-alpha.0",
  "description": "Token validation library written in Node.js",
  "keywords": [
    "js",
    "AAD",
    "msal",
    "oauth",
    "validation",
    "tokens"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "engines": {
    "node": "18"
  },
  "directories": {
    "test": "test"
  },
  "beachball": {
    "disallowedChangeTypes": [
      "patch",
      "minor",
      "major",
      "prerelease"
    ]
  },
  "scripts": {
    "clean": "shx rm -rf dist lib",
    "clean:coverage": "rimraf ./coverage/*",
    "lint": "cd ../../ && npm run lint:node:token",
    "lint:fix": "npm run lint -- -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:only": "npm run clean:coverage && npm run test:coverage",
    "build:common": "cd ../msal-common && npm run build",
    "build:modules": "rollup -c",
    "build:modules:watch": "rollup -cw",
    "build": "npm run clean && npm run build:modules",
    "build:all": "npm run build:common && npm run build",
    "prepack": "npm run build:all"
  },
  "bugs": {
    "url": "https://github.com/AzureAD/microsoft-authentication-library-for-js/issues"
  },
  "homepage": "https://github.com/AzureAD/microsoft-authentication-library-for-js#readme",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/jest": "^27.4.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "husky": "^7.0.2",
    "jest": "^27.5.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "shx": "^0.3.3",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@azure/msal-common": "^8.0.0",
    "axios": "^0.24.0",
    "jose": "^4.9.2"
  }
}
